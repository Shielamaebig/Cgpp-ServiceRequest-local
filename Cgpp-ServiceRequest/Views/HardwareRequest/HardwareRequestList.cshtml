                            <label for="">Unit Name</label>
                            <select class="form-control " name="unitTypeId" id="unitTypeId">
                            </select>
                        </div>
                        <div class="col-md-4">
                            <label for=""> Brand Name</label>
                            <input type="text" class="form-control " name="brandName" id="brandName" required />
                        </div>
                        <div class="col-md-4">
                            <label for=""> Model Name</label>
                            <input type="text" class="form-control" name="modelName" id="modelName" required />
                        </div>
                        <div class="col-md-4">
                            <label for=""> Any Desk :</label>
                            <input type="text" class="form-control " name="anyDesk" id="anyDesk" required />
                        </div>
                        <div class="col-md-4">
                            <label for=""> Tel No. :</label>
                            <input type="text" class="form-control " name="telNumber" id="telNumber" required />
                        </div>
                        <div class="row mt-3">
                            <label for="FileName" class="col-md-1 col-form-label ">Subject : </label>
                            <div class="col-md-11">
                                <input type="text" class="form-control ms-4" id="documentLabel" name="FileName" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="" class=" mb-1">Description</label>
                            <textarea class="form-control" id="description" name="description" style="height: 80px;"> </textarea>
                        </div>
                        <input type="hidden" id="id" name="id" />
                    </div>
                    <div class="modal-footer">
