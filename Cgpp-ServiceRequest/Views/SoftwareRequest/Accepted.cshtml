
@{
    ViewBag.Title = "Accepted";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (User.IsInRole("DivisionApprover"))
{
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title d-flex justify-content-between mx-2 fw-semibold">
                        Software Request List
                    </h4>
                    <h6 class="card-subtitle"></h6>
                    <div class="table-responsive">
                        <table id="softwareRequestList" class="table table-condensed display">
                            <thead>
                                <tr>
                                    <th>Ticket</th>
                                    <th>Date Posted</th>
                                    <th>Reported By</th>
                                    <th>Software Category</th>
                                    <th>Status</th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@if (User.IsInRole("DepartmentApprover"))
{
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title d-flex justify-content-between mx-2">
                        Software Request List
                    </h4>
                    <h6 class="card-subtitle"></h6>
                    <div class="table-responsive">
                        <table id="softwareRequestList2" class="table table-condensed display">
                            <thead>
                                <tr>
                                    <th>Ticket</th>
                                    <th>Date Posted</th>
                                    <th>Reported By</th>
                                    <th>Software Category</th>
