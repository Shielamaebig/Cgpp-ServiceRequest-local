
@{
    ViewBag.Title = "Activity";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4 class="card-title d-flex justify-content-between mx-2 fw-semibold">
    Activity Logs
</h4>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title d-flex justify-content-between mx-2 fw-semibold">
                </h4>
                <div class="table-responsive">
                    <table id="useractivity" class="table table-condensed display">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Log</th>
                                <th>Date</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts {
    <script>
        $(document).ready(function () {
            activityUsers()
        });
    </script>
}

